push 10
makeArray
jumpFrame fillArray
jumpFrame printArray
jumpBack

fillArray:
	pushFrom -1 # array at 0
	pushFrom 0
	arrayLength # length at 1
	push 0 # counter at 2

	loop0Start:
		pushFrom 2
		pushFrom 1
		isSame
		If
		Jump loop0End

		pushFrom 2
		pushFrom 2
		pushFrom 0
		incRef
		writeToRef

		pushFrom 2
		push 1
		mathAddInt
		WriteTo 2

		Jump loop0Start
	loop0End:
	jumpBack
printArray:
	pushFrom -1 # array at 0
	pushFrom 0
	arrayLength # length at 1
	push 0 # counter at 2

	loop1Start:
		pushFrom 2
		pushFrom 1
		isSame
		If
		Jump loop1End

		pushFrom 2
		pushFrom 0
		incRef
		deref
		writeInt
		push '\n'
		writeChar

		pushFrom 2
		push 1
		mathAddInt
		WriteTo 2

		Jump loop1Start
	loop1End:
	jumpBack
